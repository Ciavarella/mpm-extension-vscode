{
  "name": "mpm",
  "displayName": "Music Per Minute",
  "description": "Mpm or Music Per Minute is a Visual Studio Code extention that will play or pause the music on Spotify based on how fast you type. For each character you type you will be granted 1 second of playtime on Spotify. If you sign up with Spotify you can go to your settings and change the the amount of seconds you get per keypress. If your time runs out your music will be paused. Don't worry, if you keep writing code the music will start to play again.",
  "version": "1.0.0",
  "publisher": "Victor",
  "engines": {
    "vscode": "^1.30.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.mpm"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.mpm",
        "title": "Music Per Minute"
      }
    ]
  },
  "scripts": {
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.42",
    "@types/node": "^8.10.45",
    "typescript": "^3.4.3",
    "vscode": "^1.1.33"
  },
  "dependencies": {
    "dotenv": "^6.2.0",
    "node-fetch": "^2.3.0",
    "username": "^4.1.0"
  },
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Ciavarella/mpm-extension-vscode.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Ciavarella/mpm-extension-vscode/issues"
  },
  "homepage": "https://github.com/Ciavarella/mpm-extension-vscode#readme"
}
