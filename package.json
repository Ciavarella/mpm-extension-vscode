{
  "name": "mpm",
  "displayName": "Music Per Minute",
  "description": "Plays music based on how fast you type",
  "version": "1.1.0",
  "publisher": "Victor",
  "author": "",
  "engines": {
    "vscode": "^1.30.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.mpm"
  ],
  "main": "./out/extension.js",
  "icon": "logo.png",
  "contributes": {
    "commands": [
      {
        "command": "extension.mpm",
        "title": "Music Per Minute"
      },
      {
        "command": "extension.mpm.stop",
        "title": "Stop Music Per Minute"
      }
    ],
    "configuration": {
      "title": "Music per minute",
      "properties": {
        "mpm.enabled": {
          "default": false,
          "type": "boolean",
          "description": "Enable to activate Music per minute!"
        },
        "mpm.hardMode": {
          "default": false,
          "type": "boolean",
          "description": "Music per minute HARDMODE!"
        },
        "mpm.keypress": {
          "default": 1,
          "type": "number",
          "description": "Amount of seconds / keypress"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "@types/dotenv": "^6.1.1",
    "@types/mocha": "^2.2.42",
    "@types/node": "^10.12.21",
    "@types/node-fetch": "^2.3.2",
    "@types/username": "^3.0.0",
    "prettier": "^1.16.4",
    "prettier-tslint": "^0.4.2",
    "tslint": "^5.12.1",
    "tslint-config-prettier": "^1.17.0",
    "typescript": "^3.4.3",
    "vscode": "^1.1.33"
  },
  "dependencies": {
    "dotenv": "^6.2.0",
    "node-fetch": "^2.3.0",
    "username": "^4.1.0"
  },
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Ciavarella/mpm-extension-vscode.git"
  },
  "keywords": [],
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Ciavarella/mpm-extension-vscode/issues"
  },
  "homepage": "https://github.com/Ciavarella/mpm-extension-vscode#readme"
}
